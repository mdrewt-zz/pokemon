<!DOCTYPE html>
<html>
  <head>
    <title>Pokemon</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <header>
      <% if current_user %>
        Logged in as <%= current_user.username %>.
        <%= link_to 'Log Out', logout_path %>
      <% end %>
    </header>
    <%= yield %>
  </body>
</html>
